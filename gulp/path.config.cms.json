{
	"public": "./deploy/web",
	"dist": "app/themes/wbsl/dist/",

	"browserSync": {
		"proxy": "http://local.wbsl.com"
	},

	"js": {
		"dest": "app/themes/wbsl/dist/js"
	},

	"scss": {
		"dest": "app/themes/wbsl/dist/css"
	},

	"cssFonts": {
		"dest": "app/themes/wbsl/dist/css"
	},

	"images": {
		"dest": "app/themes/wbsl/dist/images"
	},

	"fonts": {
		"dest": "app/themes/wbsl/dist/fonts"
	},

	"sprites": {
		"dest": "app/themes/wbsl/dist/images"
	},

	"svgs": {
		"dest": "app/themes/wbsl/dist/images"
	},

	"symbols": {
		"dest": "app/themes/wbsl/dist/images",
		"fileDest": "../deploy/web/app/themes/wbsl/templates/includes"
	},

	"tags": {
		"src": "./deploy/web/app/themes/wbsl/templates/wrapper/_layout.twig",
		"dest": "./deploy/web/app/themes/wbsl/templates/wrapper/",
		"css": "/dist/css/",
		"js": "/dist/js/"
	},

	"critical": {
		"templates": "./deploy/fractal/06-pages",
		"urlBase": "./deploy/public/",
		"outputBase": "./deploy/fractal/06-pages",
		"paths": [
			{
				"input": {
					"url": "http://local.wbsl.com/",
					"source": "home/home.twig"
				},
				"output": {
					"dist": "./home"
				}
			}
		]
	}
}
